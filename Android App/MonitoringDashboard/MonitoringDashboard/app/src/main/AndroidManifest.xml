<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ibm.monitoringdashboard">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Intro"></activity>
        <activity
            android:name=".tables.TableExchange"
            android:label="@string/title_activity_table_exchange" />
        <activity
            android:name=".tables.TableRecharge"
            android:label="@string/title_activity_table_recharge" />
        <activity
            android:name=".tables.TableActivation"
            android:label="@string/title_activity_table_activation" />
        <activity
            android:name=".charts.ChartRecharge"
            android:label="@string/title_activity_chart_recharge" />
        <activity
            android:name=".charts.ChartActivation"
            android:label="@string/title_activity_chart_activation" />
        <activity android:name=".tables.TableMFP" />
        <activity android:name=".Homepage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>